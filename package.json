{
	"name": "tbcp-native-desktop",
	"version": "0.0.1",
	"license": "AGPL-3.0",
	"private": true,
	"readme": "https://gitlab.com/the-bootcamp-project/frameworks/native-desktop/-/blob/main/README.md",
	"description": "Framework for a modern Web Application, with Electron, TypeScript, Svelte, TailwindCSS and Webpack",
	"keywords": [
		"How to",
		"Build",
		"Publish",
		"Modern",
		"Modern",
		"Native",
		"Desktop Application",
		"Electron",
		"Svelte",
		"TailwindCSS",
		"TypeScript",
		"Webpack",
		"Boilerplate",
		"Step-by-Step"
	],
	"homepage": "https://gitlab.com/the-bootcamp-project/frameworks/native-desktop",
	"repository": "https://gitlab.com/the-bootcamp-project/frameworks/native-desktop.git",
	"bugs": {
		"url": "https://gitlab.com/the-bootcamp-project/frameworks/native-desktop/-/issues",
		"email": "contributors@bootcamp-project.com"
	},
	"author": "Bootcamp contributors <contributors@bootcamp-project.com>",
	"maintainers": [
		"Bootcamp contributors <contributors@bootcamp-project.com>",
		"John Ollhorn <john@ollhorn.de> (https://ollhorn.de)"
	],
	"contributors": [
		"John Ollhorn <john@ollhorn.de> (https://ollhorn.de)"
	],
	"browserslist": [
		"defaults"
	],
	"main": "./build/main.js",
	"scripts": {
		"clean": "rimraf ./node_modules build dist artifacts yarn.lock",
		"pre:submodules:docs": "git submodule add -b main --name documentation https://gitlab.com/the-bootcamp-project/frameworks/docs.git docs",
		"pre:submodules:i18n": "git submodule add -b main --name i18n https://gitlab.com/the-bootcamp-project/libraries/i18n.git i18n",
		"pre:submodules:init": "git submodule update --init",
		"pre:submodules:merge": "git submodule update --recursive --remote --merge",
		"preinstall": "yarn run clean",
		"dev": "webpack serve --node-env development --mode development --config webpack.electron.ts",
		"********": "--------------------------------------------------------------------------------------------------------------",
		"bundle-main": "webpack --node-env production --mode production --config webpack.main.ts --progress --no-hot --no-watch",
		"bundle-preload": "webpack --node-env production --mode production --config webpack.preload.ts --progress --no-hot --no-watch",
		"bundle-all": "webpack --node-env production --mode production --config webpack.electron.ts --progress --no-hot --no-watch && yarn bundle-main && yarn bundle-preload",
		"*******": "--------------------------------------------------------------------------------------------------------------",
		"build": "electron ./bundle/main.js",
		"******": "--------------------------------------------------------------------------------------------------------------",
		"measure-bundle": "webpack --node-env production --mode production --config webpack.electron.ts --progress --no-hot --no-watch --profile --json > stats.json && webpack-bundle-analyzer stats.json",
		"test": "mocha './tests/**/*.test.js'"
	},
